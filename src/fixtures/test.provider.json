{
  "name": "test",
  "defaultService": "default",
  "services": [
    {
      "id": "default",
      "baseUrl": "https://superface.test"
    }
  ],
  "parameters": [
    {
      "name": "param_no_default",
      "description": "Required test parameter"
    },
    {
      "name": "param_default",
      "description": "Optional test parameter",
      "default": "default value"
    }
  ],
  "securitySchemes": [
    {
      "id": "api_key",
      "type": "apiKey",
      "in": "header",
      "name": "X-API-KEY"
    },
    {
      "id": "basic",
      "type": "http",
      "scheme": "basic"
    },
    {
      "id": "bearer_token",
      "type": "http",
      "scheme": "bearer",
      "bearerFormat": "JWT"
    },
    {
      "id": "digest",
      "type": "http",
      "scheme": "digest",
      "statusCode": 401,
      "authorizationHeader": "Authorization",
      "challengeHeader": "www-authenticate"
    }
  ]
}