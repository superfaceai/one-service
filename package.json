{
  "name": "one-service",
  "version": "0.0.0",
  "description": "HTTP Service able to execute Superface capabilities",
  "main": "dist/index.js",
  "repository": "https://github.com/superfaceai/one-service",
  "author": "Superface Team <hello@superface.ai>",
  "license": "MIT",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json --outDir dist",
    "build:watch": "yarn build --watch",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -c src/",
    "format:fix": "prettier --write src/",
    "start": "./bin/server",
    "start:dev": "concurrently \"yarn build:watch\" \"nodemon -w dist dist/index.js\""
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^6.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
